{
  "name": "mechanize",
  "version": "1.3.1",
  "description": "Automate interaction with websites (web scraping)",
  "keywords": [
    "dom",
    "scraper",
    "javascript"
  ],
  "homepage": "https://github.com/srveit/mechanize-js/wiki",
  "repository": {
    "type": "git",
    "url": "git@github.com:srveit/mechanize-js.git"
  },
  "bugs": {
    "email": "steve@veitconsulting.com",
    "url": "https://github.com/srveit/mechanize-js/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Stephen R. Veit",
    "email": "steve@veitconsulting.com",
    "url": "http://veitconsulting.com"
  },
  "contributors": [
    {
      "name": "ä½è—¤"
    },
    {
      "name": "Dan Rahmel"
    },
    {
      "name": "Anders Hjelm"
    }
  ],
  "main": "./lib/mechanize",
  "bin": {},
  "man": [],
  "directories": {
    "lib": "./lib/mechanize"
  },
  "config": {},
  "scripts": {
    "docs": "jsdoc --configure .jsdocrc.json",
    "lint": "eslint . --ext .js,.json",
    "lint-markdown": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "test": "jest --detectOpenHandles",
    "test:unit": "vitest"
  },
  "watch": {
    "lint": {
      "patterns": [
        "{lib,spec}/*.js"
      ],
      "quiet": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageProvider": "v8",
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "clover",
      "json",
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 75
      }
    }
  },
  "dependencies": {
    "jsdom": "^24.0.0",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.7",
    "tough-cookie": "^4.1.3",
    "unescape": "^1.0.1",
    "windows-1252": "^1.1.0"
  },
  "devDependencies": {
    "@jest/reporters": "^27.5.1",
    "ajv": "^8.10.0",
    "ajv-keywords": "^5.1.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "github:standard/eslint-config-standard",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-n": "^15.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "express": "^4.17.3",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-html-reporter": "^3.4.2",
    "jsdoc": "^3.6.10",
    "markdownlint-cli2": "^0.4.0",
    "standard": "^16.0.4",
    "vitest": "^1.5.1"
  },
  "type": "module",
  "overrides": {},
  "bundledDependencies": [],
  "engines": {
    "node": ">= 18.0.0"
  },
  "bundleDependencies": []
}
